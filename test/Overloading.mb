class Point
{
    public x int
    public y int

    public func construct()
    {
        set(0, 0)
    }

    public func construct(x int, y int)
    {
        set(x, y)
    }

    public func set(x, y int)
    {
        self.x = x
        self.y = y
    }

    public func +=(v Point) Point
    {
        self.x += v.x
        self.y += v.y

        return self
    }
}
