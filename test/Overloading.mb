class Point
{
    x int
    y int

    func construct()
    {
        set(0, 0)
    }

    func construct(x int, y int)
    {
        set(x, y)
    }

    func set(x, y int)
    {
        self.x = x
        self.y = y
    }

    func +=(v Point) Point
    {
        self.x += v.x
        self.y += v.y

        return self
    }
}
