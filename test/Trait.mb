trait Animal
{
    name String

    public func construct(name String)
    {
        self.name = name
    }

    public func noise()
    {
        print("I'm {self.name}!")
    }
}

class Sheep extends Animal
{
    func shout()
    {
        print(self.noise())
    }
}

var sheep = Sheep("Roxy")
sheep.shout()
